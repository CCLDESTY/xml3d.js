<?xml version="1.0" encoding="UTF-8"?>
<project name="xml3d-contrib-module" default="concat" basedir=".">
    <description>
            All external libraries
    </description>

    <filelist dir="." id="sourceFiles">
        <file name="./glu.js" />
        <file name="./glMatrix.js" />
    </filelist>

    <target name="concat">
        <echo message="Merging ${ant.project.name}"/>
        <concat destfile="${build.path}/${ant.project.name}.js" eol="unix" append="true">
            <filelist refid="sourceFiles" />
        </concat>
    </target>

</project>
